{
  "name": "arwa",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "________________ BUILD AND RUN ________________": "",
    "postinstall": "npm run prepare",
    "build": "cross-env NODE_ENV=production next build",
    "dev": "next",
    "start:dev": "next",
    "start:prod": "cross-env NODE_ENV=production npx env-cmd --silent next start",
    "analyze": "cross-env ANALYZE=true next build",
    "________________ FORMAT AND LINT ________________": "",
    "ts:check": "tsc --noEmit",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "check": "npm run ts:check",
    "________________ OTHER ________________": "",
    "update": "npx ncu -u && npx rimraf node_modules package-lock.json && npm i --legacy-peer-deps",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.5.3",
    "@rainbow-me/rainbowkit": "^1.1.1",
    "@sentry/nextjs": "^7.72.0",
    "@vercel/og": "^0.5.17",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "next": "13.5.3",
    "next-compose-plugins": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.2",
    "react-i18next": "^13.2.2",
    "sass": "^1.69.0",
    "sharp": "^0.32.6",
    "viem": "^1.15.4",
    "wagmi": "^1.4.3",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@jetbrains/eslint-config": "^5.4.1",
    "@types/node": "^20.7.2",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "cross-env": "^7.0.3",
    "eslint": "8.50.0",
    "eslint-config-next": "^13.5.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-comments": "^1.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "npm-check-updates": "^16.14.4",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
